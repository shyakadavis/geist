{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"ignore": [
			"node_modules",
			".svelte-kit",
			"build",
			"vite.config.js.timestamp-*",
			"vite.config.ts.timestamp-*"
		],
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"ignore": [
			"node_modules",
			".svelte-kit",
			"build",
			"vite.config.js.timestamp-*",
			"vite.config.ts.timestamp-*"
		],
		"enabled": true
	}
}
