{
	"$schema": "https://unpkg.com/jsrepo@1.17.2/schemas/registry-config.json",
	"dirs": ["./src/lib/components", "./src/lib/assets", "./src"],
	"includeBlocks": [],
	"includeCategories": ["ui", "lib", "icons"],
	"doNotListBlocks": [],
